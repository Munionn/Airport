{
  "name": "airport-orm",
  "version": "1.0.0",
  "description": "Custom TypeScript ORM for Airport Management System",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "migrate:run": "ts-node src/cli/migrate.ts run",
    "migrate:rollback": "ts-node src/cli/migrate.ts rollback",
    "migrate:generate": "ts-node src/cli/migrate.ts generate"
  },
  "keywords": [
    "orm",
    "typescript",
    "postgresql",
    "airport",
    "database"
  ],
  "author": "Poliansky Mikhail",
  "license": "MIT",
  "dependencies": {
    "pg": "^8.16.3",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
